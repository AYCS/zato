      <xsd:element name="zato_service_invoke_request">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="id" type="xsd:int"/>
            <xsd:element name="payload" type="xsd:string"/>
            <xsd:element name="data_format" type="xsd:string"/>
            <xsd:element name="transport" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

      <xsd:element name="zato_service_invoke_response">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element ref="zato_env"/>
            <xsd:element name="item" minOccurs="0" maxOccurs="1">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="response" type="xsd:anyType"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element> 